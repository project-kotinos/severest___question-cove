<div id="clearance" class="container sign-up">
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <h2><%= t(".title") %></h2>

      <%= form_for @user do |form| %>
        <%= render partial: '/users/form', object: form %>

        <button class="btn btn-primary" type="submit"><%= t('.sign_up') %></button>
        <%= link_to t(".cancel"), sign_in_path, class: 'btn btn-default' %>
      <% end %>
    </div>
  </div>
</div>
